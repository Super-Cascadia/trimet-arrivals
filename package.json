  "jest": {
    "collectCoverageFrom": [
      "src/api/**/*.{ts,tsx}",
      "src/component/**/*.{ts,tsx}",
      "src/store/**/*.{ts,tsx}",
      "src/view/**/*.{ts,tsx}",
      "!<rootDir>/src/registerServiceWorker.ts",
      "<rootDir>/src/App.tsx"
    ]
